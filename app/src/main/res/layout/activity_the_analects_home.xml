            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/WeiLingGongTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/XianWenTitle"
            android:layout_centerHorizontal="true"
            android:text="@string/WeiLingGongTitle"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/JiShiTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/WeiLingGongTitle"
            android:layout_centerHorizontal="true"
            android:text="@string/JiShiTitle"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/YangHuoTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/JiShiTitle"
            android:layout_centerHorizontal="true"
            android:text="@string/YangHuoTitle"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/WeiZiTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/YangHuoTitle"
            android:layout_centerHorizontal="true"
            android:text="@string/WeiZiTitle"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/ZiZhangTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/WeiZiTitle"
            android:layout_centerHorizontal="true"
            android:text="@string/ZiZhangTitle"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="16dp" />

        <Button
            android:id="@+id/YaoYueTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ZiZhangTitle"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="5dp"
            android:text="@string/YaoYueTitle"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="16dp" />

    </RelativeLayout>

</ScrollView>
